{
  "name": "@youngcm2/laptop-setop",
  "version": "1.0.0",
  "description": "Fork and clone repo to ~/laptop",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"No tests configured\"",
    "start": "tsx src/index.ts",
    "build": "tsc",
    "collect": "node dist/index.js collect",
    "type-check": "tsc --noEmit",
    "lint": "echo \"No linter configured\"",
    "prepare": "bash .specify/scripts/bash/install-hooks.sh",
    "validate": "npm run type-check && npm run build && npm audit",
    "webpack": "webpack",
    "pkg:blob": "node --experimental-sea-config sea.json",
    "pkg:node": "cp $(command -v node) ./dist/laptop-setup",
    "pkg:remove-signature": "codesign --remove-signature ./dist/laptop-setup",
    "pkg:postject": "./node_modules/.bin/postject ./dist/laptop-setup NODE_SEA_BLOB ./dist/sea-prep.blob --sentinel-fuse NODE_SEA_FUSE_fce680ab2cc467b6e072b8b5df1996b2 --macho-segment-name NODE_SEA",
    "pkg:sign": "codesign --sign - ./dist/laptop-setup",
    "pkg": "npm run build && npm run webpack && npm run pkg:blob && npm run pkg:node && npm run pkg:remove-signature && npm run pkg:postject && npm run pkg:sign",
    "sea": "npm run build && node build-sea.js",
    "sea:esbuild": "npm run build && node esbuild-sea.js"
  },
  "bin": "dist/index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "volta": {
    "node": "24.12.0",
    "npm": "11.7.0"
  },
  "dependencies": {
    "@types/archiver": "7.0.0",
    "archiver": "7.0.1",
    "execa": "9.6.1",
    "fs-extra": "11.3.2",
    "tslib": "2.8.1",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@types/fs-extra": "11.0.4",
    "@types/node": "24.10.4",
    "@types/yargs": "17.0.35",
    "esbuild": "0.27.2",
    "postject": "1.0.0-alpha.6",
    "rollup": "4.53.5",
    "ts-loader": "9.5.4",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "webpack": "5.104.0",
    "webpack-cli": "6.0.1"
  }
}
